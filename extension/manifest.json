{
    "manifest_version":2,
    "name":"C^2",
    "description":"Prompts the most recent stories from CalMatters in new tabs",
    "version":"0.1",
    
    "chrome_url_overrides":{
    "newtab":"extension.html"
},

"browser_action":{
    "default_icon":"sky.JPG"
},

"permissions":[
    "tabs"
],

"background":{
    "scripts":["background.js"],
    "persistent":false
}

}